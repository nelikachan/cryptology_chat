    <!-- http://www.semanticweb.org/quantumblockchains/crypto#Bell_theorem -->

    <owl:Class rdf:about="http://www.semanticweb.org/quantumblockchains/crypto#Bell_theorem">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/quantumblockchains/crypto#quantum_mechanics_theorem"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://purl.obolibrary.org/obo/IAO_0000136"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/quantumblockchains/crypto#quantum_state"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <obo:IAO_0000115 xml:lang="en">a statement, based on a presumption that quantum mechanics is inconsistent with local theories of hidden variables, where hidden variables are hypothetical properties of quantum particles that influence the outcome of experiments and locality means that a particle can only be influenced by its immediate environment</obo:IAO_0000115>
        <qb_pdf_link xml:lang="en">https://quantumblockchains.io/kg/Belltheorem.pdf</qb_pdf_link>
        <rdfs:comment xml:lang="en">&quot;Bell’s theorem&quot; is a collective term for a number of statements of the similar content</rdfs:comment>
        <rdfs:label xml:lang="en">Bell&apos;s theorem</rdfs:label>
    </owl:Class>
    


